{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "json": {
            "()": "json_formatter.JSONFormatter"
        }
    },
    "handlers": {
        "file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "json",
            "filename": "main.jsonli",
            "maxBytes": 10485760,
            "backupCount": 3
        }
    },
    "loggers": {
        "root": {
            "handlers": "file_handler",
            "level": "DEBUG"
        }
    }
}